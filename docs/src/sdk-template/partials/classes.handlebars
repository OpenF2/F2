<h1 class="page-header">{{name}} <small>Class</small></h1>

<div class="classDescription">
    {{{classDescription}}}
</div>

<div class="well well-sm">
    {{#if uses}}
        <div class="uses">
            Uses
            <ul class="inline commas">
                {{#uses}}
                    <li><a href="{{.}}.html">{{.}}</a></li>
                {{/uses}}
            </ul>
        </div>
    {{/if}}

    {{#if extends}}
        <div class="extends">
            Extends {{#crossLink extends}}{{/crossLink}}
        </div>
    {{/if}}

    {{#if foundAt}}
        <div class="foundat">
            Defined in: <a href="{{foundAt}}">`{{{file}}}:{{{line}}}`</a>
        </div>
    {{/if}}

    {{#if since}}
        <p>Available since {{since}}</p>
    {{/if}}
</div>

{{#if deprecated}}
    <div class="well well-sm">
        <p>
        {{#if deprecationMessage}}
            <strong>Deprecated:</strong> {{deprecationMessage}}
        {{else}}
            This class is deprecated.
        {{/if}}
        </p>
    </div>
{{/if}}

{{#is_constructor}}
    <div class="constructor">
        <h2>Constructor</h2>
        {{>method}}
    </div>
{{/is_constructor}}

<div>
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#index">Summary</a></li>

        {{#if methods}}
            <li><a data-toggle="tab" href="#methods">Methods</a></li>
        {{/if}}
        {{#if properties}}
            <li><a data-toggle="tab" href="#properties">Properties</a></li>
        {{/if}}
        {{#if attrs}}
            <li><a data-toggle="tab" href="#attrs">Attributes</a></li>
        {{/if}}
        {{#if events}}
            <li><a data-toggle="tab" href="#events">Events</a></li>
        {{/if}}
    </ul>

    <div class="tab-content">
        <div id="index" class="tab-pane active">
            {{#if methods}}
                <div class="index-section methods">
                    <h3>Methods</h3>

                    <table class="table table-bordered table-striped table-members index-list methods{{#if extends}} extends{{/if}}">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Description</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#methods}}
                            <tr class="index-item method{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
                                <td class="name">
                                    <a href="#" data-tabid="#methods" data-anchor="#method_{{name}}" data-tablink="{{name}}">
                                        {{name}}
                                        <span class="paren">(</span>
                                        {{#if params}}
                                            <ul class="params commas">
                                                {{#params}}
                                                <li>
                                                    {{#if optional}}
                                                        [{{name}}{{#if optdefault}}={{optdefault}}{{/if}}]
                                                    {{else}}
                                                        {{name}}
                                                    {{/if}}
                                                </li>
                                                {{/params}}
                                            </ul>
                                        {{/if}}
                                        <span class="paren">)</span>
                                    </a>
                                    {{#if static}}
                                        <span class="label label-default static">static</span>
                                    {{/if}}
                                    {{#if deprecated}}
                                        <span class="label label-warning deprecated">deprecated</span>
                                    {{/if}}
                                    {{#if access}}
                                        <span class="label label-info {{access}}">{{access}}</span>
                                    {{/if}}
                                </td>
                                <td>{{{description}}}</td>
                            </tr>
                            {{/methods}}
                        </tbody>
                    </table>
                </div>
            {{/if}}

            {{#if properties}}
                <div class="index-section properties">
                    <h3>Properties</h3>
                    <table class="table table-bordered table-striped table-members index-list properties{{#if extends}} extends{{/if}}">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Description</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#properties}}
                            <tr class="index-item property{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
                                <td class="name">
                                    <a href="#" data-tabid="#properties" data-anchor="#property_{{name}}" data-tablink="{{name}}">{{name}}</a>
                                    {{#if static}}
                                        <span class="label label-default static">static</span>
                                    {{/if}}
                                    {{#if deprecated}}
                                        <span class="label label-warning deprecated">deprecated</span>
                                    {{/if}}
                                </td>
                                <td>{{{description}}}</td>
                            </tr>
                            {{/properties}}
                        </tbody>
                    </table>
                </div>
            {{/if}}

            {{#if attrs}}
                <div class="index-section attrs">
                    <h3>Attributes</h3>
                    <table class="table table-bordered table-striped table-members index-list attrs{{#if extends}} extends{{/if}}">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Description</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#attrs}}
                            <tr class="index-item attr{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
                                <td class="name">
                                    <a href="#" data-tabid="#attrs" data-anchor="#attr_{{name}}" data-tablink="{{name}}">{{name}}</a>
                                </td>
                                <td>{{{description}}}</td>
                            </tr>
                            {{/attrs}}
                        </tbody>
                    </table>
                </div>
            {{/if}}

            {{#if events}}
                <div class="index-section events">
                    <h3>Events</h3>
                    <table class="table table-bordered table-striped table-members index-list events{{#if extends}} extends{{/if}}">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Description</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#events}}
                            <tr class="index-item event{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if overwritten_from}} inherited{{/if}}{{#if extended_from}} inherited{{/if}}">
                                <td class="name">
                                    <a href="#" data-tabid="#events" data-anchor="#event_{{name}}" data-tablink="{{name}}">{{name}}</a>
                                    {{#if static}}
                                        <span class="label label-default static">static</span>
                                    {{/if}}
                                    {{#if deprecated}}
                                        <span class="label label-warning deprecated">deprecated</span>
                                    {{/if}}
                                </td>
                                <td>{{{description}}}</td>
                            </tr>
                            {{/events}}
                        </tbody>
                    </table>
                </div>
            {{/if}}
        </div>

        {{#if methods}}
            <div id="methods" class="tab-pane">
                <h2 class="off-left">Methods</h2>

                {{#methods}}
                    {{>method}}
                {{/methods}}
            </div>
        {{/if}}

        {{#if properties}}
            <div id="properties" class="tab-pane">
                <h2 class="off-left">Properties</h2>

                {{#properties}}
                    {{>props}}
                {{/properties}}
            </div>
        {{/if}}

        {{#if attrs}}
            <div id="attrs" class="tab-pane">
                <h2 class="off-left">Attributes</h2>

                {{#attrs}}
                    {{>attrs}}
                {{/attrs}}
            </div>
        {{/if}}

        {{#if events}}
            <div id="events" class="tab-pane">
                <h2 class="off-left">Events</h2>

                {{#events}}
                    {{>events}}
                {{/events}}
            </div>
        {{/if}}
    </div>
</div>
