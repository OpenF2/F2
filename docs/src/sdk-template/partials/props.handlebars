<section id="properties-{{memberNameAsId}}" class="level2 {{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if extended_from}} inherited{{/if}}">
	<h3>
		{{name}}

		<span class="label type">{{#crossLink type}}{{/crossLink}}</span>

		{{#if deprecated}}
			<span class="label deprecated"{{#if deprecationMessage}} title="{{deprecationMessage}}"{{/if}}>deprecated</span>
		{{/if}}

		{{#if access}}
			<span class="label {{access}}">{{access}}</span>
		{{/if}}

		{{#if final}}
			<span class="label final">final</span>
		{{/if}}

		{{#if static}}
			<span class="label static">static</span>
		{{/if}}
	</h3>

	<p>
		{{{propertyDescription}}}
	</p>

	<div class="well well-small">
		<dl class="dl-horizontal">
			{{#if overwritten_from}}
				<dt>Inherited from</dt>
				<dd>
					<a href="{{crossLinkRaw overwritten_from/class}}#method_{{overwritten_from/name}}">
						{{overwritten_from/class}}
					</a>
				</dd>
			{{else}}
				{{#if extended_from}}
					<dt>Inherited from</dt>
					<dd><a href="{{crossLinkRaw extended_from}}#method_{{name}}">{{extended_from}}</a></dd>
				{{else}}
					{{#providedBy}}
						<dt>Provided by</dt>
						<dd><a href="../modules/{{.}}.html">{{.}}</a> module.</dd>
					{{/providedBy}}
				{{/if}}
			{{/if}}
			
			{{#if foundAt}}
				<dt>Defined in</dt>
				<dd><a href="{{foundAt}}">`{{{file}}}:{{{line}}}`</a></dd>
			{{/if}}

			{{#if deprecationMessage}}
				<dt>Deprecated</dt>
				<dd>{{deprecationMessage}}</dd>
			{{/if}}

			{{#if since}}
				<dt>Available since</dt>
				<dd>{{since}}</dd>
			{{/if}}
		</dl>
	</div>

	{{#if default}}
		<p><strong>Default:</strong> {{default}}</p>
	{{/if}}

	{{#example}}
		<h4>Example:</h4>

		<div class="example-content">
			{{{.}}}
		</div>
	{{/example}}

	{{#if subprops}}
		<h4>Sub-properties:</h4>

		<ul class="params-list">
			{{#subprops}}
				<li class="param">
					{{#if optional}}
						<code class="param-name optional">[{{name}}{{#if optdefault}}={{optdefault}}{{/if}}]</code>
						<span class="type type">{{#crossLink type}}{{/crossLink}}</span>
						<span class="label label-info" title="This property is optional.">optional</span>
					{{else}}
						<code class="param-name">{{name}}</code>
						<span class="type type">{{#crossLink type}}{{/crossLink}}</span>
					{{/if}}

					<p>
						{{{description}}}
					</p>

					{{#if subprops}}
						<ul class="params-list">
							{{#subprops}}
								<li class="param">
									{{#if optional}}
										<code class="param-name optional">[{{name}}{{#if optdefault}}={{optdefault}}{{/if}}]</code>
										<span class="label type">{{#crossLink type}}{{/crossLink}}</span>
										<span class="label label-info" title="This property is optional.">optional</span>
									{{else}}
										<code class="param-name">{{name}}</code>
										<span class="label type">{{#crossLink type}}{{/crossLink}}</span>
									{{/if}}

									<p>
										{{{description}}}
									</p>
								</li>
							{{/subprops}}
						</ul>
					{{/if}}
				</li>
			{{/subprops}}
		</ul>
	{{/if}}

	<div class="backToTop">
		<a href="javascript:;">Back to top</a>
	</div>
</section>